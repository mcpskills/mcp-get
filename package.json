    "typescript": "^5.4.5"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "claude",
    "ai",
    "package-manager",
    "cli",
    "anthropic"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/mcpskills/mcp-get"
  },
  "homepage": "https://mcpskills.dev",
