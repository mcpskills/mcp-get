{
  "name": "mcp-get",
  "version": "0.1.0",
  "description": "Package manager for MCP servers - install, update, and manage Model Context Protocol servers for Claude and other AI tools",
  "author": "MCPSkills <mcpskills2026@gmail.com>",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "mcp-get": "./bin/mcp-get.js"
  },
  "scripts": {
    "build": "bun build src/index.ts --outdir dist --target node",
    "dev": "bun run src/index.ts",
